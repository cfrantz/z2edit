  cxx_flag: "external/mingw_compiler_win64/usr/lib/gcc/x86_64-w64-mingw32.shared/4.9.4/include-fixed"
  cxx_flag: "-isystem"
  cxx_flag: "external/mingw_compiler_win64/usr/lib/gcc/x86_64-w64-mingw32.shared/4.9.4/include/c++"
